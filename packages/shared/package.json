{
  "name": "@stopfra/shared",
  "version": "1.0.0",
  "type": "module",
  "description": "Shared utilities, constants, and helpers for Stop FRA platform",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "require": "./src/index.ts",
      "types": "./src/index.ts"
    },
    "./constants": {
      "import": "./src/constants/index.ts",
      "types": "./src/constants/index.ts"
    },
    "./utils": {
      "import": "./src/utils/index.ts",
      "types": "./src/utils/index.ts"
    }
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vitest run"
  },
  "devDependencies": {
    "typescript": "^5.7.0",
    "vitest": "^3.0.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}
