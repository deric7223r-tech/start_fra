{
  "name": "@stopfra/shared",
  "version": "1.0.0",
  "type": "module",
  "description": "Shared utilities, constants, and helpers for Stop FRA platform",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "require": "./src/index.ts",
      "types": "./src/index.ts"
    },
    "./constants": {
      "import": "./src/constants/index.ts",
      "types": "./src/constants/index.ts"
    },
    "./utils": {
      "import": "./src/utils/index.ts",
      "types": "./src/utils/index.ts"
    },
    "./logger": {
      "import": "./src/logger.ts",
      "types": "./src/logger.ts"
    },
    "./services": {
      "import": "./src/services/index.ts",
      "types": "./src/services/index.ts"
    },
    "./data": {
      "import": "./src/data/index.ts",
      "types": "./src/data/index.ts"
    }
  },
  "dependencies": {
    "@stopfra/types": "workspace:*"
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vitest run"
  },
  "devDependencies": {
    "eslint": "^9.32.0",
    "typescript": "~5.9.2",
    "vitest": "^4.0.18"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}
