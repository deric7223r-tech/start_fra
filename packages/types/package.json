{
  "name": "@stopfra/types",
  "version": "1.0.0",
  "type": "module",
  "description": "Shared TypeScript types for Stop FRA platform",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "require": "./src/index.ts",
      "types": "./src/index.ts"
    },
    "./assessment": {
      "import": "./src/assessment.ts",
      "types": "./src/assessment.ts"
    },
    "./auth": {
      "import": "./src/auth.ts",
      "types": "./src/auth.ts"
    },
    "./api": {
      "import": "./src/api.ts",
      "types": "./src/api.ts"
    },
    "./workshop": {
      "import": "./src/workshop.ts",
      "types": "./src/workshop.ts"
    }
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vitest run"
  },
  "devDependencies": {
    "eslint": "^9.32.0",
    "typescript": "~5.9.2",
    "vitest": "^4.0.18"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}
